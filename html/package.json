{
  "name": "LightsOut",
  "source": "src/index.html",
  "dependencies": {
    "parcel": "^2.11.0",
    "vue": "^3.4.19"
  },
  "scripts": {
    "start": "parcel",
    "clean": "rm -rf dist/",
    "build": "npm run clean && parcel build --no-source-maps && echo now run 'npm run deploy' to copy to docs/ folder",
    "deploy": "rm -rf ../docs/* && cp -rv dist/* ../docs"
  },
  "private": true,
  "devDependencies": {
    "@parcel/transformer-vue": "^2.11.0",
    "@parcel/transformer-yaml": "^2.11.0",
    "parcel-reporter-static-files-copy": "^1.5.3"
  }
}
